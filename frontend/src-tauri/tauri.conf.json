{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "csemInsight-frontend",
  "version": "0.1.0",
  "identifier": "csemInsight",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "CSEM viz tool",
        "width": 1000,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "label": "main",
        "url": "index.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher": "Yinchu Li",
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/csemInsight"] 
  }
}
